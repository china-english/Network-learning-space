{
  "name": "palladian",
  "version": "1.1.0",
  "description": "Starter kit for creating apps with React and Redux",
  "scripts": {
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools; exit 0",
    "lint:watch": "esw webpack.config.* src tools --watch; exit 0",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "eslint": "eslint"
  },
  "private": true,
  "dependencies": {
    "babel-polyfill": "6.8.0",
    "detect-browser": "1.3.3",
    "fixed-data-table": "0.6.3",
    "fixed-data-table-2": "0.7.14",
    "intl-locales-supported": "1.0.0",
    "isomorphic-fetch": "2.2.1",
    "jwt-decode": "2.0.1",
    "material-ui": "0.16.5",
    "moment": "2.17.1",
    "object-assign": "4.1.0",
    "radium": "0.19.1",
    "rc-pagination": "1.6.1",
    "rc-tree": "1.3.9",
    "react": "15.3.2",
    "react-bootstrap": "0.30.10",
    "react-dimensions": "1.3.0",
    "react-dom": "15.3.2",
    "react-pivot": "1.18.0",
    "react-player": "0.12.1",
    "react-redux": "4.4.5",
    "react-responsive": "1.2.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "react-scroll": "1.4.4",
    "react-scrollbar": "0.5.1",
    "react-select": "1.0.0-rc.3",
    "react-slick": "0.14.5",
    "react-tap-event-plugin": "1.0.0",
    "react-title-component": "1.0.1",
    "react-tooltip": "3.2.10",
    "redux": "3.6.0",
    "redux-form": "6.3.2",
    "redux-form-material-ui": "4.1.0",
    "redux-thunk": "2.1.0",
    "slick-carousel": "1.6.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.10",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
