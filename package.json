    "coveralls": "2.11.14",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "enzyme": "2.5.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-plugin-flowtype": "^2.30.4",
